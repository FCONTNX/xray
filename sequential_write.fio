[global]
ioengine=libaio
direct=1
time_based
norandommap
group_reporting
disk_util=0
continue_on_error=all
rate_process=poisson

[disk0]
bs=1m
size=2G
filename=/dev/sdb
rw=write
iodepth=6

[disk1]
bs=1m
size=2G
filename=/dev/sdc
rw=write
iodepth=6

[disk2]
bs=1m
size=2G
filename=/dev/sdd
rw=write
iodepth=6
